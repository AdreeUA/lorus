include ../logo/logo
include ../questions-form/questions-form

mixin inner-page(data)
    -data = data || {}

    +b.SECTION.inner-page()&attributes(attributes)
        +e.HEADER.header
            +logo(logo.mainGray).inner-page__logo

        block

        if (!data.noForm)
            +questions-form.inner-page__questions

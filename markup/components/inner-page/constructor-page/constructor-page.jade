include ../inner-page
include ../../line/line
include ./block-1/block-1
include ./block-2/block-2

mixin constructor-page(data)
    +inner-page({ noForm: true })
        +b.ARTICLE.constructor-page()&attributes(attributes)
            +e.lines.inner-page__lines-wrapper
                .inner-page__lines
                    +line(line.throughCareer1).career-page__line_1
                    +line(line.throughCareer2)._dashed.career-page__line_2
                    +line(line.throughCareer3).career-page__line_3

            .inner-page__content
                +block-1
                +block-2

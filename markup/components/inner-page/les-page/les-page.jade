include ../inner-page
include ../../line/line
include ./block-1/block-1
include ./block-2/block-2
include ./block-3/block-3
include ./block-4/block-4

mixin les-page(data)
    +inner-page
        +b.ARTICLE.les-page()&attributes(attributes)
            +e.lines.inner-page__lines-wrapper
                .inner-page__lines
                    +line(line.throughLes1).les-page__line_1
                    +line(line.throughLes2).les-page__line_2
                    +line(line.throughLes3).les-page__line_3

            .inner-page__content
                +block-1
                +block-2
                +block-3
                +block-4

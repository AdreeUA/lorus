include ../academy
include ../../line/line
include ../../icon/icon
include ./block-1/block-1
include ./block-2/block-2
include ./block-3/block-3
include ./block-4/block-4
include ./block-5/block-5

mixin through-academy(data)
    +academy
        +b.through-academy()&attributes(attributes)
            +e.wrapper
                +e.lines
                    +line(line.throughAcademy1).through-academy__line_1
                    +icon({'icon': 'building', 'class': 'academy__building'})
                    +line(line.throughAcademy2)._dashed.through-academy__line_2
                    +line(line.throughAcademy3).through-academy__line_3
                    +line(line.throughAcademy4).through-academy__line_4
                +e.content
                    +block-1
                    +block-2
                    +block-3
                    +block-4
                    +block-5
